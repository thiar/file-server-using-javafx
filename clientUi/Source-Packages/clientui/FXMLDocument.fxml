                      <ListView fx:id="listUser" prefHeight="261.0" prefWidth="251.0" GridPane.halignment="LEFT" GridPane.rowIndex="4" />
                  <HBox prefHeight="35.0" prefWidth="251.0" GridPane.rowIndex="8">
