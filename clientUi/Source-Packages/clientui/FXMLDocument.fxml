<TabPane prefHeight="436.0" prefWidth="305.0" tabClosingPolicy="UNAVAILABLE" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="clientui.FXMLDocumentController">  
    
    <tabs>
